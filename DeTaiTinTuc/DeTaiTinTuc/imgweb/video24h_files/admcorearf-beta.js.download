						window.fake_browser='firefox';
						_admloadJs('//static.philacct.com/cdnflash/cdnflash_'+id+'.js');
					});
				},1000);
			}
			else{
				admicroAD.loaded.push(id);
				var dmn=document.domain+'';
				var arrZonedemo=ADS_CHECKER.getQuery('arfdemo');
				var strZonedemo='';
				try{
					if(arrZonedemo!=''){
						if(arrZonedemo==='true' || ADS_CHECKER.in_array(id,arrZonedemo.split(","))){
							strZonedemo='-demo';
						}
					}
				}catch(e){}
				var ele=document.querySelector('#'+id);
				
				var checkDomready=true;
				if(document.readyState === "interactive") {
				   console.log(ele,id,checkDomready);
				    checkDomready=false;
				}
				
				if(ADS_CHECKER.in_array(id,['ks9svot2','ks9suiud','ksnh9vc1','ksnhartx'])){
					//document.write('<div style="display:none;">show data'+id+'</div>');

