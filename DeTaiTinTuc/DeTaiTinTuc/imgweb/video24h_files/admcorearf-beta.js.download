					_admloadJs('//media1.admicro.vn/cms/arf-'+id+strZonedemo+'.min.js',function(){},function(){
						window.chkAdmblockAds=window.chkAdmblockAds||false;
						if(!window.chkAdmblockAds){
							window.chkAdmblockAds=true;
							_admloadJs('https://media1.admicro.vn/cms/Askmy.min.js',function(){
								_admloadJs('https://static.philacct.com/cms/arf-'+id+strZonedemo+'.min.js');
							})

						}else{
							_admloadJs('https://static.philacct.com/cms/arf-'+id+'.min.js');
